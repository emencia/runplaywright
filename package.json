    "playtest": "nodejs bin/playtest"
  },
  "keywords": [
    "playwright",
    "test"
  ],
  "peerDependencies": {
    "npx": "^10.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.23.1"
  },
  "files": [
    "bin"
  ],
  "bin": {
    "runtest": "bin/runtest.js",
    "playtest": "bin/playtest.js",
    "showtests": "bin/list_tests.js"
  },
  "preferGlobal": true
}