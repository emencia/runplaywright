{
  "name": "runplaywright",
  "version": "0.0.7",
  "description": "A test runner for Playwright",
  "repository": "https://github.com/emencia/runplaywright",
  "license": "MIT",
  "scripts": {
    "showtests": "nodejs bin/list_tests",
